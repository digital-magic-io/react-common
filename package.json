{
  "name": "@digital-magic/react-common",
  "version": "19.0.8",
  "description": "Common types & utilities for strictly-typed React applications written with TS",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./lib",
    "lint": "eslint .",
    "jest-clear-cache": "jest --clearCache",
    "prettier": "pretty-quick --check --pattern \"src/**/*.*(ts|tsx)\" --pattern \"test/**/*.*(ts|tsx)\"",
    "fix-prettier": "pretty-quick --pattern \"src/**/*.*(ts|tsx)\" --pattern \"test/**/*.*(ts|tsx)\"",
    "jest": "jest",
    "test": "npm run jest-clear-cache && npm run prettier && npm run jest",
    "prebuild": "npm run clean && npm run prettier",
    "postbuild": "pretty-quick --pattern \"lib/**/*.*(js|ts|jsx|tsx)\"",
    "prepare": "husky install && tsc -p .",
    "build": "tsc -p ./tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-magic-io/react-common.git"
  },
  "files": [
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "author": "Jevgeni Goloborodko <jevgeni@digital-magic.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digital-magic-io/react-common/issues"
  },
  "homepage": "https://github.com/digital-magic-io/react-common#readme",
  "keywords": [
    "typescript",
    "react"
  ],
  "tags": [
    "typescript",
    "react"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@digital-magic/ts-common-utils": "^1.0.28",
    "@hookform/resolvers": "^3.1.1",
    "@tanstack/react-query": "^4.29.14",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.13",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "axios": "^1.4.0",
    "eslint": "^8.43.0",
    "eslint-plugin-functional": "^5.0.8",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "i18next": "^23.2.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-i18next": "^13.0.0",
    "react-router-dom": "^6.13.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-object-path": "^0.1.2",
    "typescript": "^5.1.3",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "@digital-magic/ts-common-utils": ">=1.0.28",
    "@hookform/resolvers": ">=3.1.1",
    "@tanstack/react-query": ">=4.29.14",
    "axios": ">=1.4.0",
    "i18next": ">=23.2.0",
    "react": ">=18.2.0",
    "react-hook-form": ">=7.45.0",
    "react-i18next": ">=13.0.0",
    "react-router-dom": ">=6.13.0",
    "ts-object-path": ">=0.1.2",
    "zod": ">=3.21.4"
  }
}
