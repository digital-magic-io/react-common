{
  "name": "@digital-magic/react-common",
  "version": "18.2.26",
  "description": "Common types & utilities for strictly-typed React applications written with TS",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./lib",
    "lint": "eslint .",
    "jest-clear-cache": "jest --clearCache",
    "prettier": "pretty-quick --check --pattern \"src/**/*.*(ts|tsx)\" --pattern \"test/**/*.*(ts|tsx)\"",
    "fix-prettier": "pretty-quick --pattern \"src/**/*.*(ts|tsx)\" --pattern \"test/**/*.*(ts|tsx)\"",
    "jest": "jest",
    "test": "npm run jest-clear-cache && npm run prettier && npm run jest",
    "prebuild": "npm run clean && npm run prettier",
    "postbuild": "pretty-quick --pattern \"lib/**/*.*(js|ts|jsx|tsx)\"",
    "prepare": "husky install && tsc -p .",
    "build": "tsc -p ./tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-magic-io/react-common.git"
  },
  "files": [
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "author": "Jevgeni Goloborodko <jevgeni@digital-magic.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digital-magic-io/react-common/issues"
  },
  "homepage": "https://github.com/digital-magic-io/react-common#readme",
  "keywords": [
    "typescript",
    "react"
  ],
  "tags": [
    "typescript",
    "react"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@digital-magic/ts-common-utils": "^1.0.28",
    "@hookform/resolvers": "^2.9.11",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "axios": "^1.3.4",
    "eslint": "^8.34.0",
    "eslint-plugin-functional": "^5.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "i18next": "^22.4.10",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-hook-form": "^7.43.2",
    "react-i18next": "^12.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.1",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-object-path": "^0.1.2",
    "typescript": "^4.9.5",
    "zod": "^3.20.6"
  },
  "peerDependencies": {
    "@digital-magic/ts-common-utils": ">=1.0.28",
    "@hookform/resolvers": ">=2.9.11",
    "axios": ">=1.3.4",
    "i18next": ">=22.4.10",
    "react": ">=18.2.0",
    "react-hook-form": ">=7.43.2",
    "react-i18next": ">=12.2.0",
    "react-query": ">=3.39.3",
    "react-router-dom": ">=6.8.1",
    "ts-object-path": ">=0.1.2",
    "zod": ">=3.20.6"
  }
}
